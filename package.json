{
  "author": "Danil Semelenov",
  "browser": "index.min.js",
  "description": "Use CSS Modules as tagged templates in a browser and Node.js.",
  "devDependencies": {
    "eslint": "*",
    "postcss": "^7.0.14",
    "postcss-modules-local-by-default": "^2.0.4",
    "postcss-modules-parser": "^1.1.1",
    "postcss-modules-scope": "^2.0.1",
    "postcss-modules-values": "^2.0.0",
    "prettier": "*",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-virtual": "^1.0.1",
    "source-map-explorer": "^1.6.0"
  },
  "homepage": "https://github.com/sgtpep/csstag",
  "keywords": [
    "browser",
    "css",
    "css-modules",
    "esm",
    "postcss",
    "tagged-template",
    "tagged-template-literal",
    "template-literal"
  ],
  "license": "MIT",
  "name": "csstag",
  "repository": "sgtpep/csstag",
  "scripts": {
    "build": "rollup -c",
    "check-format": "npm run format -- -c --no-write",
    "format": "prettier --write '**/*.{css,html,js,json,md,yml}'",
    "lint": "eslint '**/*.js'",
    "prepublishOnly": "rollup -c",
    "test": "./test"
  },
  "version": "0.3.0"
}
